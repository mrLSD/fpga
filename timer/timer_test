v0x15d96c0_0 .var "seconds", 5 0;
v0x15d97a0_0 .var "timer_enabled", 0 0;
E_0x15751e0 .event edge, v0x15d96c0_0;
E_0x15aa660 .event edge, v0x15d9140_0;
E_0x15aa2e0/0 .event negedge, v0x15d9600_0;
E_0x15aa2e0/1 .event posedge, v0x15b1070_0;
E_0x15aa2e0 .event/or E_0x15aa2e0/0, E_0x15aa2e0/1;
    .scope S_0x1574f10;
T_0 ;
    %pushi/vec4 0, 0, 24;
    %store/vec4 v0x15d9140_0, 0, 24;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x15d96c0_0, 0, 6;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15d97a0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x1574f10;
T_1 ;
    %wait E_0x15aa2e0;
    %load/vec4 v0x15d9600_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 24;
    %assign/vec4 v0x15d9140_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x15d96c0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x15d93f0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x15d9310_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x15d9140_0;
    %addi 1, 0, 24;
    %assign/vec4 v0x15d9140_0, 0;
    %load/vec4 v0x15d9140_0;
    %parti/s 1, 23, 6;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x15d96c0_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x15d96c0_0, 0;
    %load/vec4 v0x15d96c0_0;
    %pad/u 32;
    %cmpi/e 60, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x15d96c0_0, 0;
    %load/vec4 v0x15d93f0_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x15d93f0_0, 0;
    %load/vec4 v0x15d93f0_0;
    %pad/u 32;
    %cmpi/e 60, 0, 32;
    %jmp/0xz  T_1.6, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x15d93f0_0, 0;
    %load/vec4 v0x15d9310_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x15d9310_0, 0;
    %load/vec4 v0x15d9310_0;
    %pad/u 32;
    %cmpi/e 24, 0, 32;
    %jmp/0xz  T_1.8, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x15d9310_0, 0;
T_1.8 ;
T_1.6 ;
T_1.4 ;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1574f10;
T_2 ;
    %wait E_0x15aa660;
    %load/vec4 v0x15d9140_0;
    %parti/s 4, 12, 5;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 62, 0, 6;
    %assign/vec4 v0x15d9220_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x15d9220_0;
    %parti/s 5, 0, 2;
    %load/vec4 v0x15d9220_0;
    %parti/s 1, 5, 4;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x15d9220_0, 0;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x1574f10;
T_3 ;
    %wait E_0x15751e0;
    %load/vec4 v0x15d96c0_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 6;
    %cmp/u;
    %jmp/1 T_3.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 6;
    %cmp/u;
    %jmp/1 T_3.1, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 6;
    %cmp/u;
    %jmp/1 T_3.2, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 6;
    %cmp/u;
    %jmp/1 T_3.3, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 6;
    %cmp/u;
    %jmp/1 T_3.4, 6;
    %dup/vec4;
