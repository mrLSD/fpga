#! /usr/local/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1574d90 .scope module, "main" "main" 2 4;
 .timescale 0 0;
v0x15d9970_0 .var "clk", 0 0;
v0x15d9a30_0 .net "digit_block", 5 0, v0x15d9220_0;  1 drivers
v0x15d9b00_0 .net "number", 7 0, v0x15d9520_0;  1 drivers
v0x15d9c00_0 .var "rst", 0 0;
S_0x1574f10 .scope module, "t1" "timer" 2 10, 3 3 0, S_0x1574d90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /OUTPUT 8 "number"
    .port_info 3 /OUTPUT 6 "digit_block"
v0x15b1070_0 .net "clk", 0 0, v0x15d9970_0;  1 drivers
v0x15d9140_0 .var "count", 23 0;
v0x15d9220_0 .var "digit_block", 5 0;
v0x15d9310_0 .var "hours", 5 0;
v0x15d93f0_0 .var "minutes", 5 0;
v0x15d9520_0 .var "number", 7 0;
