#! /usr/local/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x16fab90 .scope module, "main" "main" 2 3;
 .timescale 0 0;
v0x17206e0_0 .var "button", 0 0;
v0x1720780_0 .var "clk", 0 0;
v0x1720850_0 .net "key_press", 0 0, v0x17205a0_0;  1 drivers
S_0x16fad10 .scope module, "debouncer_instance" "debouncer" 2 9, 3 1 0, S_0x16fab90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "button"
    .port_info 2 /OUTPUT 1 "key_press"
L_0x1720a50 .functor XNOR 1, v0x17205a0_0, v0x1720160_0, C4<0>, C4<0>;
v0x16fe4d0_0 .net "button", 0 0, v0x17206e0_0;  1 drivers
v0x17200a0_0 .var "button_sync_0", 0 0;
v0x1720160_0 .var "button_sync_1", 0 0;
v0x1720230_0 .net "clk", 0 0, v0x1720780_0;  1 drivers
v0x17202f0_0 .var "counter", 2 0;
v0x1720420_0 .net "counter_max", 0 0, L_0x1720950;  1 drivers
v0x17204e0_0 .net "idle", 0 0, L_0x1720a50;  1 drivers
v0x17205a0_0 .var "key_press", 0 0;
E_0x16fd9b0 .event posedge, v0x1720230_0;
L_0x1720950 .reduce/and v0x17202f0_0;
    .scope S_0x16fad10;
T_0 ;
    %wait E_0x16fd9b0;
    %load/vec4 v0x16fe4d0_0;
    %inv;
    %assign/vec4 v0x17200a0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x16fad10;
T_1 ;
    %wait E_0x16fd9b0;
    %load/vec4 v0x17200a0_0;
    %assign/vec4 v0x1720160_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x16fad10;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x17205a0_0, 0, 1;
    %vpi_call 3 20 "$monitor", v0x1720230_0, " B = %b | PRESS = %b | SYNC = %b | IDEL = %b", v0x16fe4d0_0, v0x17205a0_0, v0x1720160_0, v0x17204e0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x16fad10;
T_3 ;
    %wait E_0x16fd9b0;
    %load/vec4 v0x17204e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x17202f0_0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x17202f0_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x17202f0_0, 0;
    %load/vec4 v0x1720420_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.2, 8;
    %load/vec4 v0x17205a0_0;
    %inv;
    %assign/vec4 v0x17205a0_0, 0;
T_3.2 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x16fab90;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1720780_0, 0, 1;
T_4.0 ;
    %delay 1, 0;
    %load/vec4 v0x1720780_0;
    %inv;
    %store/vec4 v0x1720780_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x16fab90;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1720780_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x17206e0_0, 0, 1;
T_5.0 ;
    %delay 3, 0;
    %load/vec4 v0x17206e0_0;
    %inv;
    %store/vec4 v0x17206e0_0, 0, 1;
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0x16fab90;
T_6 ;
    %delay 40, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
