#! /usr/local/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1694d80 .scope module, "main" "main" 2 3;
 .timescale 0 0;
v0x16baaa0_0 .var "button", 0 0;
v0x16bab40_0 .var "clk", 0 0;
v0x16bac10_0 .net "key_press", 0 0, v0x16ba960_0;  1 drivers
S_0x1694f00 .scope module, "debouncer_instance" "debouncer" 2 9, 3 1 0, S_0x1694d80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "button"
    .port_info 2 /OUTPUT 1 "key_press"
P_0x16950d0 .param/l "TIME_DELAY" 0 3 7, +C4<00000000000000000000000000000010>;
L_0x16bae10 .functor XNOR 1, v0x16ba960_0, v0x16ba520_0, C4<0>, C4<0>;
v0x1698830_0 .net "button", 0 0, v0x16baaa0_0;  1 drivers
v0x16ba460_0 .var "button_sync_0", 0 0;
v0x16ba520_0 .var "button_sync_1", 0 0;
v0x16ba5f0_0 .net "clk", 0 0, v0x16bab40_0;  1 drivers
v0x16ba6b0_0 .var "counter", 1 0;
v0x16ba7e0_0 .net "counter_max", 0 0, L_0x16bad10;  1 drivers
v0x16ba8a0_0 .net "idle", 0 0, L_0x16bae10;  1 drivers
v0x16ba960_0 .var "key_press", 0 0;
E_0x1697d60 .event posedge, v0x16ba5f0_0;
L_0x16bad10 .reduce/and v0x16ba6b0_0;
    .scope S_0x1694f00;
T_0 ;
    %wait E_0x1697d60;
    %load/vec4 v0x1698830_0;
    %inv;
    %assign/vec4 v0x16ba460_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1694f00;
T_1 ;
    %wait E_0x1697d60;
    %load/vec4 v0x16ba460_0;
    %assign/vec4 v0x16ba520_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1694f00;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x16ba960_0, 0, 1;
    %vpi_call 3 23 "$monitor", v0x16ba6b0_0, " B = %b | PRESS = %b | SYNC = %b | IDEL = %b", v0x1698830_0, v0x16ba960_0, v0x16ba520_0, v0x16ba8a0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x1694f00;
T_3 ;
    %wait E_0x1697d60;
    %load/vec4 v0x16ba8a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x16ba6b0_0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x16ba6b0_0;
    %addi 1, 0, 2;
    %assign/vec4 v0x16ba6b0_0, 0;
    %load/vec4 v0x16ba7e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.2, 8;
    %load/vec4 v0x16ba960_0;
    %inv;
    %assign/vec4 v0x16ba960_0, 0;
T_3.2 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1694d80;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x16bab40_0, 0, 1;
T_4.0 ;
    %delay 1, 0;
    %load/vec4 v0x16bab40_0;
    %inv;
    %store/vec4 v0x16bab40_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x1694d80;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x16bab40_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x16baaa0_0, 0, 1;
T_5.0 ;
    %delay 20, 0;
    %load/vec4 v0x16baaa0_0;
    %inv;
    %store/vec4 v0x16baaa0_0, 0, 1;
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0x1694d80;
T_6 ;
    %delay 300, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
