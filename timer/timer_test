#! /usr/local/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1355410 .scope module, "main" "main" 2 1;
 .timescale 0 0;
v0x1381e60_0 .var "clk", 0 0;
v0x1381f50_0 .var "pb", 0 0;
v0x1382060_0 .net "pb_out", 0 0, L_0x13821f0;  1 drivers
S_0x1355590 .scope module, "db" "debounce" 2 8, 2 25 0, S_0x1355410;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "pb_1"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /OUTPUT 1 "pb_out"
L_0x1382100 .functor NOT 1, v0x1381160_0, C4<0>, C4<0>, C4<0>;
L_0x13821f0 .functor AND 1, v0x1380b70_0, L_0x1382100, C4<1>, C4<1>;
v0x13818f0_0 .net "Q1", 0 0, v0x1380b70_0;  1 drivers
v0x13819e0_0 .net "Q2", 0 0, v0x1381160_0;  1 drivers
v0x1381aa0_0 .net "Q2_bar", 0 0, L_0x1382100;  1 drivers
v0x1381b40_0 .net "clk", 0 0, v0x1381e60_0;  1 drivers
v0x1381c10_0 .net "pb_1", 0 0, v0x1381f50_0;  1 drivers
v0x1381d00_0 .net "pb_out", 0 0, L_0x13821f0;  alias, 1 drivers
v0x1381da0_0 .net "slow_clk", 0 0, v0x1381790_0;  1 drivers
S_0x1359410 .scope module, "d1" "dff" 2 30, 2 51 0, S_0x1355590;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "d"
    .port_info 2 /OUTPUT 1 "q"
v0x1359630_0 .net "clk", 0 0, v0x1381790_0;  alias, 1 drivers
v0x1380ab0_0 .net "d", 0 0, v0x1381f50_0;  alias, 1 drivers
v0x1380b70_0 .var "q", 0 0;
v0x1380c40_0 .var "qq", 0 0;
E_0x131d6b0 .event posedge, v0x1359630_0;
S_0x1380d80 .scope module, "d2" "dff" 2 31, 2 51 0, S_0x1355590;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "d"
    .port_info 2 /OUTPUT 1 "q"
v0x1380fa0_0 .net "clk", 0 0, v0x1381790_0;  alias, 1 drivers
v0x1381090_0 .net "d", 0 0, v0x1380b70_0;  alias, 1 drivers
v0x1381160_0 .var "q", 0 0;
v0x1381230_0 .var "qq", 0 0;
S_0x1381330 .scope module, "u1" "clock_div" 2 28, 2 42 0, S_0x1355590;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /OUTPUT 1 "slow_clk"
v0x13815d0_0 .net "clk", 0 0, v0x1381e60_0;  alias, 1 drivers
v0x13816b0_0 .var "counter", 2 0;
v0x1381790_0 .var "slow_clk", 0 0;
E_0x1381570 .event posedge, v0x13815d0_0;
    .scope S_0x1381330;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x13816b0_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x1381330;
T_1 ;
    %wait E_0x1381570;
    %pushi/vec4 3, 0, 32;
    %load/vec4 v0x13816b0_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %flag_mov 8, 5;
    %jmp/0 T_1.0, 8;
    %pushi/vec4 0, 0, 3;
    %jmp/1 T_1.1, 8;
T_1.0 ; End of true expr.
    %load/vec4 v0x13816b0_0;
    %addi 1, 0, 3;
    %jmp/0 T_1.1, 8;
 ; End of false expr.
    %blend;
T_1.1;
    %assign/vec4 v0x13816b0_0, 0;
    %load/vec4 v0x13816b0_0;
    %pad/u 32;
    %cmpi/u 2, 0, 32;
    %flag_mov 8, 5;
    %jmp/0 T_1.2, 8;
    %pushi/vec4 0, 0, 1;
    %jmp/1 T_1.3, 8;
T_1.2 ; End of true expr.
    %pushi/vec4 1, 0, 1;
    %jmp/0 T_1.3, 8;
 ; End of false expr.
    %blend;
T_1.3;
    %assign/vec4 v0x1381790_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1359410;
T_2 ;
    %wait E_0x131d6b0;
    %load/vec4 v0x1380ab0_0;
    %assign/vec4 v0x1380b70_0, 0;
    %load/vec4 v0x1380b70_0;
    %assign/vec4 v0x1380c40_0, 0;
    %vpi_call 2 62 "$display", "DFF: %b <= %b | %b", v0x1380b70_0, v0x1380ab0_0, v0x1380c40_0 {0 0 0};
    %jmp T_2;
    .thread T_2;
    .scope S_0x1380d80;
T_3 ;
    %wait E_0x131d6b0;
    %load/vec4 v0x1381090_0;
    %assign/vec4 v0x1381160_0, 0;
    %load/vec4 v0x1381160_0;
    %assign/vec4 v0x1381230_0, 0;
    %vpi_call 2 62 "$display", "DFF: %b <= %b | %b", v0x1381160_0, v0x1381090_0, v0x1381230_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_0x1355590;
T_4 ;
    %vpi_call 2 34 "$monitor", $time, " CLK: %b | PB: %b | %b == %b", v0x1381da0_0, v0x1381c10_0, v0x13818f0_0, v0x13819e0_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x1355410;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1381e60_0, 0, 1;
T_5.0 ;
    %delay 1, 0;
    %load/vec4 v0x1381e60_0;
    %inv;
    %store/vec4 v0x1381e60_0, 0, 1;
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0x1355410;
T_6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1381e60_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1381f50_0, 0, 1;
T_6.0 ;
    %delay 3, 0;
    %load/vec4 v0x1381f50_0;
    %inv;
    %store/vec4 v0x1381f50_0, 0, 1;
    %jmp T_6.0;
    %end;
    .thread T_6;
    .scope S_0x1355410;
T_7 ;
    %delay 40, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
