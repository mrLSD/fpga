    %assign/vec4 v0x15d9520_0, 0;
    %jmp T_3.10;
T_3.10 ;
    %pop/vec4 1;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x1574d90;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15d9970_0, 0, 1;
    %vpi_call 2 14 "$monitor", $time, "  %b", v0x15d9a30_0 {0 0 0};
T_4.0 ;
    %delay 1, 0;
    %load/vec4 v0x15d9970_0;
    %inv;
    %store/vec4 v0x15d9970_0, 0, 1;
    %jmp T_4.0;
