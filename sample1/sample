#! /usr/local/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1868df0 .scope module, "sample1" "sample1" 2 1;
 .timescale 0 0;
v0x1868f70_0 .var "clk", 0 0;
v0x18c1880_0 .var "count", 27 0;
v0x18c1960_0 .var "en", 0 0;
E_0x18a0a70 .event posedge, v0x1868f70_0;
    .scope S_0x1868df0;
T_0 ;
    %delay 5, 0;
    %load/vec4 v0x1868f70_0;
    %inv;
    %store/vec4 v0x1868f70_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1868df0;
T_1 ;
    %wait E_0x18a0a70;
    %load/vec4 v0x18c1880_0;
    %addi 1, 0, 28;
    %store/vec4 v0x18c1880_0, 0, 28;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1868df0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1868f70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18c1960_0, 0, 1;
    %vpi_call 2 15 "$display", "Started..." {0 0 0};
    %vpi_call 2 16 "$monitor", $time, v0x18c1880_0 {0 0 0};
    %vpi_call 2 17 "$monitor", $time, " en ", v0x18c1960_0 {0 0 0};
    %delay 10, 0;
    %load/vec4 v0x18c1960_0;
    %pad/u 3;
    %addi 2, 0, 3;
    %pad/u 1;
    %store/vec4 v0x18c1960_0, 0, 1;
    %delay 20, 0;
    %load/vec4 v0x18c1960_0;
    %pad/u 3;
    %addi 3, 0, 3;
    %pad/u 1;
    %store/vec4 v0x18c1960_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "main.v";
